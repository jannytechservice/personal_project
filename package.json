{
  "name": "@admiin-com/web-template",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "clean": "rm -rf node_modules **/node_modules && rm -rf yarn.lock && rm -rf lerna-debug.log && yarn cache clean && touch yarn.lock",
    "test:all": "yarn nx run-many --all --target=test",
    "lint:all": "yarn nx run-many --all --target=lint",
    "transpile:appsync:resolvers": "yarn esbuild --bundle --sourcemap=inline --sources-content=false --target=esnext --platform=node --format=esm --external:@aws-appsync/utils --outdir=apps/backend/src/test/appsync/resolvers/bundled apps/backend/src/appsync/resolvers/*.ts",
    "test:backend": "yarn run transpile:appsync:resolvers && yarn nx test backend",
    "publish-ci": "lerna publish from-package -y --no-verify-access",
    "postinstall": "husky install",
    "build:commit": "yarn nx affected -t build --base=main~1 --head=main",
    "esbuild": "npx esbuild",
    "codegen": "cd ./apps/backend/src/appsync && npx @aws-amplify/cli codegen && ls && cp -R graphql ../../../../libs/graphql/src/lib  && cp API.ts ../../../../libs/graphql/src/lib  && cp -R graphql ../layers/dependencyLayer/opt  && cp API.ts ../layers/dependencyLayer/opt",
    "deploy:dev:approve-all": "yarn nx deploy backend --all --require-approval=never --profile admiin",
    "deploy:dev:approve-backend": "yarn nx deploy backend ADMdevLayerStack ADMdevDatabaseStack ADMdevAuthStack ADMdevAppSyncAPIStack ADMdevMediaStorageStack --require-approval=never --profile admiin",
    "deploy:dev:layer": "yarn nx deploy backend ADMdevLayerStack --require-approval=never --profile admiin",
    "output": "cp output.json ./apps/react-app/src/app && cp output.json ./apps/backoffice-app/src/app",
    "deploy:dev:api": "yarn nx deploy backend ADMdevAppSyncApiStack --require-approval=never --profile admiin",
    "deploy:dev:auth": "yarn nx deploy backend ADMdevAuthStack --require-approval=never --profile admiin",
    "deploy:dev:frontend": "yarn nx lint react-app && yarn nx build react-app && yarn nx deploy backend ADMdevFrontendStack --require-approval=never --profile admiin",
    "deploy:dev:db": "yarn nx deploy backend ADMdevDatabaseStack --require-approval=never --profile admiin",
    "deploy:dev:services": "yarn nx deploy backend ADMdevServicesStack --require-approval=never --profile admiin",
    "deploy:dev:rest": "yarn nx deploy backend ADMdevRestApiStack --require-approval=never --profile admiin",
    "deploy:dev:pinpoint": "yarn nx deploy backend ADMdevPinpointStack --require-approval=never --profile admiin",
    "deploy:prod:approve-all": "yarn nx deploy backend --all --require-approval=never --profile admiin",
    "deploy:prod:services": "yarn nx deploy backend ADMprodServicesStack --require-approval=never --profile admiin",
    "deploy:prod:frontend": "yarn nx lint react-app && yarn nx build react-app --skip-nx-cache && yarn nx deploy backend ADMprodFrontendStack --require-approval=never --profile admiin",
    "sso": "aws sso login --profile admiin",
    "generate-cert": "openssl req -x509 -newkey rsa:2048 -nodes -keyout key.pem -out cert.pem -days 365 -subj \"/CN=localhost\""
  },
